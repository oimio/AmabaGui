<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:npui="urn:import:ch.amaba.client.ui">

	<ui:style>
		.small {
			font-size: 9px;
		}
		
		gwt-RadioButton {
			font-size: 9px;
		}
	</ui:style>
	<ui:with type="ch.amaba.client.NameTokens"
		field="nameTokens"></ui:with>
	<g:VerticalPanel styleName='{style.small}'>
		<g:Label>HIT LIST</g:Label>
		<g:DecoratorPanel>
			<g:FlexTable ui:field="htmlPanel"></g:FlexTable>
		</g:DecoratorPanel>
	</g:VerticalPanel>

</ui:UiBinder> 